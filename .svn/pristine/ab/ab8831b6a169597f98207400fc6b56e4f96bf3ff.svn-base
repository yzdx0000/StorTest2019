def install_parastor(tar_path, parastor_name, cabinet_name="cabinet", cabinet_start_num="1",
                     cabinet_end_num="2", cabinet_height="42"):
    global driver
    log.info("start to install parastor")

    path_list = tar_path.split('/')[1:]

    # 点击部署新系统
    find_element_by_xpath("//*[@id='ofs_select_storage_system_deploy_new']", check=True).click()

    # 点击浏览
    find_element_by_xpath("//*[@id='ofs_install_browse_file']").click()

    # 选择路径
    for dirname in path_list:
        if dirname == path_list[-1]:
            find_element_by_xpath("//*[contains(text(),'%s')]" % dirname).click()
            break
        find_element_by_xpath("//*[contains(text(),'%s')]/../../span" % dirname).click()

    # 点击确定路径
    find_element_by_xpath("//*[@id='ofs_file_explorer_window-btn-0']").click()

    # 点击确定选择安装文件
    find_element_by_xpath("//*[@id='ofs_install_select_install_file_widow-btn-0']").click()

    # 等待解压完成
    check_element_done(time_out=120)

    # 输入存储系统名称
    find_element_by_xpath("//*[@id='ofs_install_system_name_id']", js_send_keys=parastor_name)

    # 点击下一步
    find_element_by_xpath("//*[@id='ofs_install_system_desc_next_button']").click()

    # 点击添加机柜
    find_element_by_xpath("//*[@id='ofs_install_rack_info_addRack']").click()

    # 勾选批量添加机柜、填写相关信息
    find_element_by_xpath("//*[@id='ofs_install_batch_add_rack_checkbox_id']").send_keys(Keys.SPACE)
    find_element_by_xpath("//*[@id='ofs_install_rack_info_rack_name_prefix_id']", js_send_keys=cabinet_name)
    find_element_by_xpath("//*[@id='ofs_install_rack_info_rack_start_number_id']", js_send_keys=cabinet_start_num)
    find_element_by_xpath("//*[@id='ofs_install_rack_info_rack_end_number_id']", js_send_keys=cabinet_end_num)
    Select(find_element_by_xpath("//*[@id='ofs_install_rack_info_rack_height_id']")).select_by_value(cabinet_height)

    # 点击确定添加机柜
    find_element_by_xpath("//*[@id='ofs_install_rack_info_addRack_id-btn-0']").click()

    # 点击下一步
    find_element_by_xpath("//*[@id='ofs_install_rack_info_button_next']").click()

    # 点击添加节点
    find_element_by_xpath("//*[@id='ofs_install_nodes_info_addNode']").click()

    return