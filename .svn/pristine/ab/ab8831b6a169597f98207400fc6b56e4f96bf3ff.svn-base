def all_events_search_function(happen_time, desc_or_level, start_time=None, end_time=None,
                               description=None, level=None):
    """
    所有事件界面搜索功能
    :param happen_time:     unlimited(不限) or custom(自定义)
    :param desc_or_level:   desc(描述) or level(等级)
    :param start_time:      2018-12-15 11:19:22
    :param end_time:        2018-12-15 11:19:22
    :param description:     （输入搜索的关键字）
    :param level:           None（全部）or 0（提示）or 1（警告）or 2（严重）or 3（致命）
    :return:
    """
    global driver
    log.info("all events search function")

    # 定位到所有事件
    locate_all_events()

    # 选择发生时间
    Select(find_element_by_xpath("//*[@id='ofs_all_event_search_start_time_label_id']")).select_by_value(happen_time)

    if start_time:
        find_element_by_xpath("//*[@id='ofs_all_event_search_start_time_start_id']", js_send_keys=start_time)

    if end_time:
        find_element_by_xpath("//*[@id='ofs_all_event_search_start_time_end_id']", js_send_keys=end_time)

    # 选择描述或者等级搜索
    Select(find_element_by_xpath("//*[@id='ofs_all_event_search_type_id']")).select_by_value(desc_or_level)

    if description:
        find_element_by_xpath("//*[@id='ofs_all_event_search_content_id']", js_send_keys=description)

    if level:
        find_element_by_xpath("//*[@id='ofs_all_event_search_content_id']").click()
        level_name = find_element_by_xpath("//*[@id='ofs_all_event_search']//ul/li[%s]" % (int(level) + 2)).text
        find_element_by_xpath("//*[@id='ofs_all_event_search_content_id']", js_send_keys=level_name)
        find_element_by_xpath("//*[@id='ofs_all_event_search_content_id']/../input[@type='hidden']", js_send_keys=level)
        find_element_by_xpath("//*[@id='ofs_all_event_search_content_id']/..//li[@data='%s']" % level).click()

    # 点击搜索
    find_element_by_xpath("//*[@id='ofs_all_event_search']//i").click()

    # 如果搜索结果为空,暂无数据
    time.sleep(2)
    obj_search_list = driver.find_elements_by_xpath(
        "//*[@id='ofs_all_event_grid_id']//div[@class='kuma-uxtable-body-scroll kuma-uxtable-no-v-scroll']/div/div")
    if obj_search_list:
        log.info("no search result")
        return

    # 检查按时间搜索的正确性
    if happen_time == "custom":
        # 第一页第一条搜索项
        first_menu_time = find_element_by_xpath("//*[@id='ofs_all_event_table']//li[1]/div/div[4]/div").text

        # 最后一页最后一条搜索项
        find_element_by_xpath("//*[@id='ofs_all_event_table']//ul[contains(@class, 'kuma-page')]/li[last()-1]").click()
        last_menu_time = find_element_by_xpath(
            "//*[@id='ofs_all_event_table']//ul/child::node()[1]/div/div[4]/div").text

        if check_time_earlier_or_later(start_time, first_menu_time) > 0 > check_time_earlier_or_later(
                end_time, last_menu_time):
            log.info("check event search by time right")

        else:
            log.info("failed %s %s %s %s" % (start_time, first_menu_time, last_menu_time, end_time))
            raise Exception("check event search by time failed")

    # 检查按 描述 搜索的正确性
    if desc_or_level == "desc" and description:
        xpath = "//*[@id='ofs_all_event_table']//li/div/div[3]/div/a"
        implicit_wait(xpath)
        obj_list = driver.find_elements_by_xpath(xpath)
        for obj in obj_list:
            if description != obj.text:
                log.info("%s %s" % (description, obj.text))
                raise Exception("check event search by description failed")
        log.info("check event search by description right")

    # 检查按 等级 搜索的正确性
    if desc_or_level == "level" and level:
        xpath = "//*[@id='ofs_all_event_table']//li/div/div[2]/div/font"
        implicit_wait(xpath)
        obj_list = driver.find_elements_by_xpath(xpath)
        for obj in obj_list:
            global level_name
            if level_name != obj.text:
                log.info("%s %s" % (level_name, obj.text))
                raise Exception("check event search by level failed")
        log.info("check event search by level right")

    # 检查搜索完成
    check_element_done()

    return

def check_node_operate_bandwidth_iops():
    # 检查节点运维界面带宽、IOPS不为N/A
    global driver
    log.info("check bandwidth and iops")

    # 定位到节点运维
    locate_node_operation()

    # 获取每个节点的带宽和iops标签对象
    bandwidth_xpath = "//*[@id='node_grid']//div[@class='kuma-uxtable-row-cells']/div[last()-3]"
    iops_xpath = "//*[@id='node_grid']//div[@class='kuma-uxtable-row-cells']/div[last()-2]"
    implicit_wait(bandwidth_xpath)
    bandwidth_obj_list = driver.find_elements_by_xpath(bandwidth_xpath)
    iops_obj_list = driver.find_elements_by_xpath(iops_xpath)

    # 检查bandwidth不为N/A
    for i in range(len(bandwidth_obj_list)):
        if "B/s" not in bandwidth_obj_list[i].text:
            raise Exception("check bandwidth is N/A failed and bandwidth:%s" % bandwidth_obj_list[i].text)
        if ":" not in iops_obj_list[i].text:
            raise Exception("check iops is N/A failed and iops:%s" % bandwidth_obj_list[i].text)

    log.info("check bandwidth and iops right")

    return

def search_function_for_node_operation(search_option,
                                       name_value=None, healthy_state_value=None, running_state_value=None):
    """
    节点运维搜索功能
    :param search_option: eg name healthy_state running_state
    :param name_value:xxx
    :param healthy_state_value:None(全部) normal（正常） abnormal（异常） unknown（获取中）
    :param running_state_value:None（全部） normal（正常） abnormal（异常） deleting（删除中）
                                reinstalling （重装中） shutdown （关闭） maintaining （维护中）
                                prepare_maintaining （下线中）prepare_online（上线中） zombie（待移除） unknown（获取中）
    :return:
    """
    log.info("search function for node operation")

    # 定位的到节点运维
    locate_node_operation()
    xpath_select = "//*[@id='node_search_type_id']"
    xpath_search = "//*[@id='node_search_content_id']"

    Select(find_element_by_xpath(xpath_select)).select_by_value(search_option)

    # 按名称搜索
    if name_value:
        find_element_by_xpath(xpath_search, js_send_keys=name_value)

    # 按健康状态搜索
    if healthy_state_value:
        find_element_by_xpath(xpath_search + "/../input[1]", js_send_keys=healthy_state_value)
        find_element_by_xpath(xpath_search).click()
        healthy_state_text = find_element_by_xpath(xpath_search + "/../ul/li[@data='%s']" % healthy_state_value).text
        find_element_by_xpath(xpath_search, js_send_keys=healthy_state_text)
        check_element_done_by_click()

    # 按运行状态搜索
    if running_state_value:
        find_element_by_xpath(xpath_search + "/../input[1]", js_send_keys=running_state_value)
        find_element_by_xpath(xpath_search).click()
        running_state_text = find_element_by_xpath(xpath_search + "/../ul/li[@data='%s']" % running_state_value).text
        find_element_by_xpath(xpath_search, js_send_keys=running_state_text)
        check_element_done_by_click()

    # 点击搜索
    find_element_by_xpath(xpath_search + "/../i").click()

    # 检查搜索完成
    check_element_done()

