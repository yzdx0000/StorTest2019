                    # 修改maxdata
                    cmd = ('grep -c lun %s' % (vdb_xml,))
                    res, lun_num = commands.getstatusoutput(cmd)
                    maxdata = int(lun_num) * 5 + 4
                    cmd = ("sed -r -i 's/maxdata=[0-9]+[a-zA-Z]+,/maxdata=%sG,/g' %s" % (str(maxdata), vdb_xml))
