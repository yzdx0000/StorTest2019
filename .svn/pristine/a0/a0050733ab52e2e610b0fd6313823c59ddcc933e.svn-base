            xfer1 = random.randint(1, 256)
            xfer2 = random.randint(1, 256)
            xfer3 = random.randint(1, 256)
            xfer4 = random.randint(1, 256)
            # 读写百分比
            seed = 100
            xfer_pcnt1 = random.randint(1, (seed / 2))
