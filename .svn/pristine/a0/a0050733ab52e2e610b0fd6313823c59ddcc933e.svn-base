    def analysis_vip(self, vip=None):
        '''
        date    :   2018-06-22
        Description :   解析vip列表为单个vip
        param   :   vip : vip
        return  :   vip_list
        '''
        vips = []
        if ',' in vip:
            vip = vip.split(',')
        for tmp_vip in vip:
            if '-' in tmp_vip:
                ip = re.match('(\d+.\d+.\d+).(\d+)-(\d+)', tmp_vip)
                for j in range(int(ip.group(2)), int(ip.group(3)) + 1):
                    vips.append(ip.group(1) + '.' + str(j))
            else:
                vips.append(tmp_vip)
        return vips

