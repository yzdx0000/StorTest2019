<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<testconfig>
    <subnet_info>
        <ip_families>
            <ip_family>IPv4</ip_family>
            <ip_family>IPv6</ip_family>
        </ip_families>
        <svips>
            <svip>20.2.40.100</svip>
        </svips>
        <svips>
            <svip>20.2.40.101</svip>
        </svips>

        <subnet_masks>
            <subnet_mask>22</subnet_mask>
        </subnet_masks>

        <subnet_gateways>
            <subnet_gateway>10.2.43.254</subnet_gateway>
        </subnet_gateways>

        <network_interfaces>
            <network_interface>ens192</network_interface>         # 一张网卡名
        </network_interfaces>
        <network_interfaces>
            <network_interface>ens192,ens224</network_interface>  # 多张网卡名
        </network_interfaces>

        <mtus>
            <mtu>1500</mtu>
        </mtus>
    </subnet_info>

    <vip_address_pool_info>
        <domain_names>
            <domain_name>www.domaindomain.com</domain_name>
        </domain_names>
        <domain_names>
            <domain_name>www.nastest2.com</domain_name>
        </domain_names>

        <vip_addresses>
            <vip_addresse>20.2.40.1-12</vip_addresse>                           # 连续型vip池地址
        </vip_addresses>
        <vip_addresses>
            <vip_addresse>20.2.40.11-20</vip_addresse>                          # 连续型vip池地址，需要跟上一个有冲突
        </vip_addresses>
        <vip_addresses>
            <vip_addresse>20.2.40.21,20.2.40.22,20.2.40.23,20.2.40.24,20.2.40.25</vip_addresse> # 离散型vip池地址
        </vip_addresses>

        <supported_protocols>
            <supported_protocol>NAS</supported_protocol>
            <supported_protocol>ISCSI</supported_protocol>
        </supported_protocols>

        <allocation_methods>
            <allocation_method>STATIC</allocation_method>
            <allocation_method>DYNAMIC</allocation_method>
        </allocation_methods>

        <load_balance_policies>
            <load_balance_policy>LB_ROUND_ROBIN</load_balance_policy>
            <load_balance_policy>LB_CONNECTION_COUNT</load_balance_policy>
            <load_balance_policy>LB_THROUGHPUT</load_balance_policy>
            <load_balance_policy>LB_CPU_USAGE</load_balance_policy>
        </load_balance_policies>

        <ip_failover_policies>
            <ip_failover_policy>IF_ROUND_ROBIN</ip_failover_policy>
            <ip_failover_policy>IF_CONNECTION_COUNT</ip_failover_policy>
            <ip_failover_policy>IF_THROUGHPUT</ip_failover_policy>
            <ip_failover_policy>IF_CPU_USAGE</ip_failover_policy>
        </ip_failover_policies>

        <rebalance_policies>
            <rebalance_policy>RB_DISABLED</rebalance_policy>
            <rebalance_policy>RB_AUTOMATIC</rebalance_policy>
        </rebalance_policies>
    </vip_address_pool_info>

    <nfs_client_info>
        <nfs_client_ip>
            <nfs_client_ip>10.2.42.135</nfs_client_ip>
            <nfs_client_ip>10.2.40.78</nfs_client_ip>
            <nfs_client_ip>10.2.40.79</nfs_client_ip>
            <nfs_client_ip>10.2.42.139</nfs_client_ip>
        </nfs_client_ip>
    </nfs_client_info>

    <ftp_client_info>
        <ftp_client_ip>
            <ftp_client_ip>10.2.42.135</ftp_client_ip>
            <ftp_client_ip>10.2.42.131</ftp_client_ip>
        </ftp_client_ip>
    </ftp_client_info>

    <ldap_server_info>
        <ldap_ip_addresses>
            <ldap_ip_addresses>10.2.41.181</ldap_ip_addresses>
            <ldap_ip_addresses>10.2.41.239</ldap_ip_addresses>
        </ldap_ip_addresses>
        <ldap_base_dn>
            <ldap_base_dn>dc=test,dc=com</ldap_base_dn>
            <ldap_base_dn>dc=ldap-pdc,dc=com</ldap_base_dn>
        </ldap_base_dn>
        <ldap_bind_dn>
            <ldap_bind_dn>cn=root,dc=test,dc=com</ldap_bind_dn>
            <ldap_bind_dn>cn=Manager,dc=ldap-pdc,dc=com</ldap_bind_dn>
        </ldap_bind_dn>
        <ldap_bind_passwd>
            <ldap_bind_passwd>111111</ldap_bind_passwd>
            <ldap_bind_passwd>111111</ldap_bind_passwd>
        </ldap_bind_passwd>
        <ldap_domain_passwd>
            <ldap_domain_passwd>111111</ldap_domain_passwd>
            <ldap_domain_passwd>111111</ldap_domain_passwd>
        </ldap_domain_passwd>
        <ldap_user_search_path>
            <ldap_user_search_path>ou=People,dc=test,dc=com</ldap_user_search_path>
            <ldap_user_search_path>ou=People,dc=ldap-pdc,dc=com</ldap_user_search_path>
        </ldap_user_search_path>
        <ldap_group_search_path>
            <ldap_group_search_path>ou=Group,dc=test,dc=com</ldap_group_search_path>
            <ldap_group_search_path>ou=Group,dc=ldap-pdc,dc=com</ldap_group_search_path>
        </ldap_group_search_path>
        <ldap_user_name>
            <ldap_user_name>ldapuser1</ldap_user_name>   # 可使用su登录的用户
            <ldap_user_name>ldapuser2</ldap_user_name>   # 有附属组的用户
            <ldap_user_name>用户@a</ldap_user_name>      # 有特殊字符与中文的用户
        </ldap_user_name>
        <ldap_user_passwd>
            <ldap_user_passwd>111111</ldap_user_passwd>    # ldap第一个用户的密码
            <ldap_user_passwd>111111</ldap_user_passwd>
            <ldap_user_passwd>111111</ldap_user_passwd>
        </ldap_user_passwd>
        <ldap_group_name>
            <ldap_group_name>ldapgroup1</ldap_group_name>   # ldap_user_name第一个用户的用户组
            <ldap_group_name>用户组1</ldap_group_name>      # 有中文的用户组
            <ldap_group_name>group@a1</ldap_group_name>     # 有特殊字符的用户组
        </ldap_group_name>
    </ldap_server_info>

    <ad_server_info>
        <ad_domain_names>
            <ad_domain_name>adtest.com</ad_domain_name>
        </ad_domain_names>
        <ad_dns_addresses>
            <ad_dns_address>10.2.41.251</ad_dns_address>
