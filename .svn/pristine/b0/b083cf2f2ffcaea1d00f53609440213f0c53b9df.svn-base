            <ad_group>AD_USER_1st_GROUP</ad_group>
            <ad_group>AD_USER_2nd_GROUP</ad_group>
        </ad_groups>
        <ad_user_pws>
            <ad_user_pw>111111</ad_user_pw>
        </ad_user_pws>
    </ad_server_info>

    <nis_server_info>
        <nis_domain_names>
            <nis_domain_name>nistest</nis_domain_name>
        </nis_domain_names>
        <nis_ip_addresses>
            <nis_ip_address>10.2.41.247</nis_ip_address>
        </nis_ip_addresses>
        <nis_users>
            <nis_user>nisuser1</nis_user>
