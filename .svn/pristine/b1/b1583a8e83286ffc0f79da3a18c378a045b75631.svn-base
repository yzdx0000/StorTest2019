#!/usr/bin/env python
"此脚本是根据当前脚本所在路径来获得conf的绝对路径"
import os
def getoutputpath():
    current_path = os.path.abspath('')
    current_uppath= os.path.dirname(current_path)
    outputpath=os.path.join(current_uppath,'S3output')
    return outputpath

if __name__=="__main__":
   out=getoutputpath()
   cmdfile = os.path.join(out,'cmdfile')
   print cmdfile


