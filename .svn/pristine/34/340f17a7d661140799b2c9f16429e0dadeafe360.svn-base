<workload name="s3-sample" description="sample benchmark for s3">

  <storage type="s3" config="accesskey=00003TU3ZNVAENV900003TU3ZNUZMVRQ;secretkey=001a8fa96ae48ab20c4a5bfe445fb6ab028fc148;endpoint=http://www.dws.com:20480;path_style_access=true;timeout=120000;" />

  <workflow>

    <workstage name="init">
