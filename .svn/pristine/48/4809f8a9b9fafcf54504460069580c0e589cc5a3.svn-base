        if len(obj) != 0:
            # 判断时间在脚本执行后
            time2 = find_element_by_xpath("//*[starts-with(text(),'%s')]/../../../div[4]/div" % event_name).text
            if check_time_earlier_or_later(time1, time2) > 0:
