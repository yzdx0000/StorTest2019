#Xstor安装包的包名，不包含后缀(该项已无需配置)
[xstor_package]
pack_name=parastor-3.0.0-centos7.5-feature_XSTOR1000-Epic-2018-04_1a6b201_20181106_210422-2-1

#新添加节点的配置文件名
[add_node_conf]
conf=/home/deploy/deploy_config_1.xml

#create_node_pool
[create_node_pool]
node_ids=1,2,3
replica_num=3
stripe_width=3
disk_parity_num=0
node_parity_num=2
name=nodepool1


#create_subnet
#network_interface:数据网网卡，数据网故障会用到
#vv_ip用于寻找业务网网卡的ip,也就是需要填一个要作为的业务网卡现有的实际物理ip
[create_subnet]
name=subnet1
sv_ip=15.1.1.150
sv_ip2=15.1.1.112
sv_ip3=15.1.1.113
vv_ip=15.1.1.20
vv_ip2=15.1.1.21
vv_ip3=15.1.1.22
mask=24
gate_way=15.1.1.254
network_interface=ens9f0

#add_vip_address_pool
#vip  这里只填单个vip
#vips   填一个vip地址段
[add_vip_address_pool]
domain_name=sugon.com
vip=15.1.1.151
vips=15.1.1.151-156

#add_initiator(启动器的配置)
#iqn：主机1的iqn(必填)
#iqn1：主机2的iqn(执行修复用例必填:因为修复用例有涉及2个主机端
[add_initiator]
iqn=iqn.1995-21.com.sugon
iqn1=iqn.1996-21.com.sugon
alias=sugon.initiator
alias1=sugon.initiator1

#CHAP
[CHAP]
CHAP_user=sugon
CHAP_passwd=qwer123
CHAP_updated_passwd=update2018
error_user=error_user

#update_param
[update_param]
section=MGR
name=mgcd_auto_mount_flag
current=0

#timeout_decorator
[timeout]
second=30000000000000000000000000000000000
maxretry=1
use_signals=True
timeout_exception=StopIteration

#xmlconfig_file(XML文件名)
[xml_confile]
filename=x1000_test_config.xml

#Exsi_arg(Esxi主机的参数配置(针对集群节点是虚拟机的情况))
[esxi]
Esxi_ips = 10.2.40.15
esxi_user=root
esxi_passwd=parastor;123

#repair_test(手动更改修复用例超时时间设置)
[timeout]
disk_timeout=30
node_timeout=60

#运行vdbench过程中，等待时间过后再开始诸如磁盘故障类的故障操作
#remove_disk  作故障前等待时间(比如拔盘，插盘，先等待若干秒)
#interval  反复拔盘插盘的间隔时间
[wait_time]
remove_disk=60
interval_disk=100

#machine_type(集群节点机器类型：  vir  or    phy)
[machine]
machine_type=vir

#vdbench参数设置
[vdbench]
mix_xfersize1 = (1k,20,3k,35,16k,20,127k,25)
unmix_xfersize1 = (4k)
offset=2048
maxdata=24G
range=2G
threads=32
interval=1
iorate=100
seekpct=0
elapsed=1200

#环境检查要传的参数
#jnl_rep:日志副本数
#node_num:集群内节点数
[env]
jnl_rep=3
free_jnl_num=0
node_num=3

#空间预留参数
#thin_lun_reserve_equation:预留空间公式(此项计算出来的thin_lun_reserver是实际的预留空间值)
#thin_lun_reserve:预留空间大小
#replica_num(*必配*:此项为空间管理用例的配置项，需要和存储池的副本数一致)
[reserve]
thin_lun_reserve_equation=256M*1024 * (1024 + 20)
thin_lun_reserve=273678336
replica_num=3
thin_lun_warn=80

