#函数名称：uploadid_init
#函数作用：初始化多段上传
#输入：oossip,bucketname,objectname,certificateid
#输出：uploadid
def init(oossip,bucketname,objectname,certificateid):
	print "**********************************************************************"
	print '初始化多段上传'
	#步骤一：初始化多段上传，将结果打印到file1中
	cmd1='curl -i http://%s:20480/%s/%s?uploads -XPOST -H "Authorization: AWS4-HMAC-SHA256 Credential=%s"' %(oossip,bucketname,objectname,certificateid)
	print cmd1
	(status1,output1)=commands.getstatusoutput(cmd1)
	#print output1
	print 'status1 is %s:' % status1
	outputpath=getoutputpath_lyz.getoutputpath()
	out=os.system('cd '+outputpath+';touch fileuploadid')
	fileuploadidallpath=os.path.join(outputpath,'fileuploadid')
	print 'fileuploadidallpath is %s' % fileuploadidallpath
	print output1
	with open(fileuploadidallpath,'w') as f:
		f.write(output1)
	#print "status is %s,putput is %s" % (status1,output1)
	re=_checkresult_lyz.checkresult(status1,output1)
	if re==-1:
		return -1
	upid=_xmlanalyzeUploadid.analyze_xml()
	print "uploadId is %s" % upid
	return upid

#函数名称：canclesplit
#函数功能：取消多段上传
#函数参数：oossip,bucketname,objectname,uploadId,certificateid
def canclesplit(oossip,bucketname,objectname,uploadId,certificateid):
	cmdcancle='curl -i http://%s:20480/%s/%s?uploadId=%s -XDELETE -H "Authorization: AWS4-HMAC-SHA256 Credential=%s"' %(oossip,bucketname,objectname,uploadId,certificateid)
	print 'cmdcancle is %s' %cmdcancle
	(statuscancle,outputcancle)=commands.getstatusoutput(cmdcancle)
	re=_checkresult_lyz.checkresult(statuscancle,outputcancle)
	if re == -1:
		return -1
	print '************取消多段上传成功****************'

#uploadPeriod负责分段上传，将文件的每一个段都传上去，注意段的个数不能超过10个
#进行该步骤前要保证要上传的文件已经一批能够split -b num filename -d surobjectname分割过
#如果参数中surobjectname是test，num=2，则上传的段名称必须是test00，test02
def uploadoncePeriod(oossip,bucketname,objectname,certificateid,surobjectpathdir,surobjectname,num,uploadid):
	print "**************************************************************************"
	print "上传第一段"
	num=int(num)
	#print 'num is %s' %num
	#上传第一段
	comsurobjectname0 = surobjectpathdir + '/' + surobjectname + str(0)+str(0)
	cmdforup = 'curl -i http://%s:20480/%s/%s?partNumber=%s\&uploadId=%s -XPUT -H "Authorization: AWS4-HMAC-SHA256 Credential=%s" -T %s' %(oossip,bucketname,objectname,'1',uploadid,certificateid,comsurobjectname0)
	(statusforup, outputforup) = commands.getstatusoutput(cmdforup)
	print outputforup
	re=_checkresult_lyz.checkresult(statusforup, outputforup)
	if re == -1:
		return -1
	print '***********分段上传成功上传第一段*************'

def uploadotherPeriod(oossip,bucketname,objectname,certificateid,surobjectpathdir,surobjectname,num,uploadid):
	num=int(num)
	for i in range(1,num):
		#print 'range(num) is %s' %range(num)
		#print 'i is %s' %i
		comsurobjectname=surobjectpathdir+'/'+surobjectname+str(0)+str(i)
		print comsurobjectname
		partNumber=str(i+1)
		#print partNumber
		cmd2='curl -i http://%s:20480/%s/%s?partNumber=%s\&uploadId=%s -XPUT -H "Authorization: AWS4-HMAC-SHA256 Credential=%s" -T %s' %(oossip,bucketname,objectname,partNumber,uploadid,certificateid,comsurobjectname)
		print cmd2
		(status2,output2)=commands.getstatusoutput(cmd2)
		#print 'status2 is %s' % status2
		print output2
		#print 'status2 is %s' %status2
		if output2.find('200 OK')<0:
			print '取消多段上传成功，段无法上传'
		else:
			print 'error#####################################################'
			print '取消多段上传后，段依然上传成功'
			return -1



#函数名称：combinePeriod
#函数作用：合并段
def combinePeriod(oossip,bucketname,objectname,uploadid,certificateid,num):
	print "********************************************************************************"
	print "合并段"
	cmd3=' curl -i http://%s:20480/%s/%s?uploadId=%s -XPOST -H "Authorization: AWS4-HMAC-SHA256 Credential=%s" -d "<CompleteMultipartUpload>' %(oossip,bucketname,objectname,uploadid,certificateid)
	num=int(num)
	for i in range(num):
		cmdme='<Part><PartNumber>%s</PartNumber><ETag>etag1</ETag></Part>' % str(i+1)
		cmd3=cmd3+cmdme
	cmd3=cmd3+'</CompleteMultipartUpload>"'
	print cmd3
	(status3,output3)=commands.getstatusoutput(cmd3)
	print output3
	re=_checkresult_lyz.checkresult(status3,output3)
	if re == -1:
		return -1

def cancleupObjectBySplit(oossip,bucketname,objectname,certificateid,surobjectpathdir,surobjectname,num):
	uploadid=init(oossip,bucketname,objectname,certificateid)
	uploadoncePeriod(oossip, bucketname, objectname, certificateid, surobjectpathdir, surobjectname, num, uploadid)
	canclesplit(oossip, bucketname, objectname, uploadid, certificateid)
	uploadotherPeriod(oossip,bucketname,objectname,certificateid,surobjectpathdir,surobjectname,num,uploadid)
	#combinePeriod(oossip,bucketname,objectname,uploadid,certificateid,num)

