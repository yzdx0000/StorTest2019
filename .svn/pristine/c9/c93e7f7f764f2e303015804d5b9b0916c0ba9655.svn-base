def createManyAccount(mgrip,accountname,accountemailfre,accountnum):
	accountnum=int(accountnum)
	allaccountnameall=[]
	for i in range(accountnum):
		accountnameall=accountname+str(i)
		print 'accountname is %s' %accountnameall
		allaccountnameall.append(accountnameall)
		accountemail=accountemailfre+str(i)+'@sugon.com'
		rec=_create_account.acc_add(mgrip,accountnameall,accountemail)
		if rec==-1:
			return -1
	print '**************************'
	print 'allaccountname is %s'%allaccountnameall
	return allaccountnameall
