#!/usr/bin/python
import os,sys
import commands
import xml.dom.minidom

def result2file(out):
	f=open('output','w')
	f.write(out)
	f.close()
def get_xml():
	f = open('output')
        lines = f.readlines()
        count = 0
        for line in lines:
                if line.startswith('<?xml'):
                        break
                else:
                        count = count + 1
        xml_lst = lines[count:]
        f.close()
        xml_string = ''.join(xml_lst)
        #print xml_string
        return xml_string

def analyze_xml():
        xml_string = get_xml()
        xml_info = xml.dom.minidom.parseString(xml_string)
        contents_info = xml_info.getElementsByTagName('Contents')
	objnames=[]
        for content_info in contents_info:
            Name = content_info.getElementsByTagName('Key')[0].firstChild.nodeValue
            objnames.append(Name)
	return objnames

